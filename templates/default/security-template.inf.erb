[System Access]
<% node['security_policy']['password_policy'].each do |setting, value| %>
  <% unless value.nil? -%>
<%= setting %> = <%= value %>
  <% end %>
<% end %>
<% node['security_policy']['account_lockout_policy'].each do |setting, value| %>
  <% unless value.nil? -%>
<%= setting %> = <%= value %>
  <% end %>
<% end %>

[Privilege Rights]
<% node['security_policy']['rights'].each do |setting, value| %>
    <% unless value.nil? -%>
<%= setting %> = <%= value %>
    <% end %>
<% end %>
